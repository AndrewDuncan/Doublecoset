    booktitle    = {Computational and statistical group theory. Proceedings of the AMS special session on geometric group theory, Las Vegas, NV, USA, April 21--22, 2001 and the AMS special session on computational group theory, Hoboken, NJ, USA, April 28--29, 2001},
    isbn         = {0-8218-3158-5},
    pages        = {83-95},
    publisher    = {Providence, RI: American Mathematical Society (AMS)},
    abstract     = {Given a finitely generated subgroup $K$ of a free group $F$ it is possible to associate a certain multigraph called the core graph of $K$, denoted by $\Gamma_K$, as shown in work of {\it J. R. Stallings} [Invent. Math. 71, 551-565 (1983; Zbl 0521.20013)]. The core graph of $K$ has some natural properties. Conversely, given a graph $\Gamma$ with such properties, the current author proves that there is a finitely generated subgroup $K$ of $F_n$ (the free group of rank $n$, where $n$ is an integer related to the degrees of the vertices of $\Gamma$) such that the core graph of $K$ is $\Gamma$ (assigning labels and directions appropriately). A subgroup $K$ of a group $G$ is malnormal in $G$ if $g^{-1}Kg\cap K=1$ for all $g\in G\setminus K$. Using ideas of {\it J. R. Stallings} [loc. cit.] and {\it I. Kapovich} and {\it A. Myasnikov} [J. Algebra 248, No. 2, 608-668 (2002; Zbl 1001.20015)] the author gives an interesting characterization of finitely generated malnormal subgroups of a free group of rank $n$ in terms of the Kronecker product $\Gamma_{g^{-1}Kg}\times\Gamma_K$ for certain $g\in F_n\setminus K$. The author uses this information to show that if $K$ is a finitely generated subgroup of $F_n$ and if the density $D$ and valency $V$ of $\Gamma_K$ satisfy $D>\sqrt n$ and $V\geq(nD-n)/(D^2-n)$ then $K$ is not malnormal. In turn, by introducing a certain probability space, the author shows that finitely generated malnormal subgroups of free groups are quite common even though ``non-obvious'' examples are difficult to come by.},
    reviewer     = {Martyn Dixon (Tuscaloosa)},
    identifier   = {01836317},
}

@article{Kapovich97,
author="Kapovich, Ilya",
title="{Amalgamated products and the Howson property.}",
language="English",
journal="Can. Math. Bull. ",
volume=40,
number=3,
pages="330-340",
year=1997,
doi={10.4153/CMB-1997-039-3},
abstract="{The author studies amalgamated products and HNN-extensions of groups
    with respect to the Howson property. A group $G$ is said to have the Howson
    property, or to be a Howson group, if the intersection of two finitely
    generated subgroups is again finitely generated. He extends known results by
    considering a class of groups which he calls $\text{class}(Q)$: A group $G$
    belong to $\text{class}(Q)$, if $G$ is word hyperbolic in the sense of M.
    Gromov and any finitely generated subgroup of $G$ is quasiconvex in $G$.
    Thus $\text{class}(Q)$ contains finitely generated free groups and
    fundamental groups of closed hyperbolic surfaces. It is shown that all
    groups in $(Q)$ are Howson groups.\par The main result is that a maximal
    cyclic subgroup in a torsion-free group $G$ in $\text{class}(Q)$, is a Burns
    subgroup of $G$. This implies that if $A$ is a torsion-free group in $(Q)$,
    $U$, $V$ maximal cyclic subgroups of $A$, and $B$ a Howson group, then the
    amalgamated product $A*_UB$ and the HNN-extension $\langle A,t\mid
    U^t=V\rangle$ have the Howson property. The author also exhibits a class of
    word hyperbolic groups which do not have the Howson property. Details and
    precise definitions are too difficult to be given here.}",
reviewer="{S.Andreadakis (Athens)}",
keywords="{amalgamated products; HNN-extensions; Howson property; intersections
    of finitely generated subgroups; finitely generated free groups; fundamental
    groups of closed hyperbolic surfaces; Howson groups; Burns subgroups;
    maximal cyclic subgroups; word hyperbolic groups}",
classmath="{*20E06 (Free products and generalizations (group theory))
20E07 (Subgroup theorems (group theory))
20F65 (Geometric group theory)
}",
}


